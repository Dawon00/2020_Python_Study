gp = float(input("Grade points을 입력하세요: "))

if gp > 4.0 :
    print('A+')
elif gp == 4.0 :
    print('A')
elif 3.7 <= gp < 4.0:
    print('A-')
elif 3.3 <= gp < 3.7:
    print('B+')
elif 3.0 <= gp < 3.3:
    print('B')
elif 2.7 <= gp < 3.0:
    print('B-')
elif 2.3 <= gp < 2.7:
    print('C+')
elif 2.0 <= gp < 2.3:
    print('C')
elif 1.7 <= gp < 2.0:
    print('C-')
elif 1.3 <= gp < 1.7:
    print('D+')
elif 1.0 <= gp < 1.3:
    print('D')
elif gp < 1.0 :
    print('F')
