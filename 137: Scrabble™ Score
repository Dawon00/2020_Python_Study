#scrabble score 

score = {"b": 3 , "c": 3 , "d": 2 ,
         "f": 4 , "g": 2 , "h": 4 ,
          "j": 8 , "k": 5 , 
         "m": 3 , "p": 3 ,
         "q": 10,   
         "v": 4 , "w": 4 , "x": 8 ,
         "y": 4 , "z": 10}

def scrabble_score(word):
    total = 0;
    for i in word:
        i = i.lower(); 
        total = total + score[i];
    return total;

word = input("Word score: ");

print(scrabble_score(word));
